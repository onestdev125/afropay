{
    "name": "afropay/laravel",
    "description": "A Laravel package for Afropay payment gateway integration",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Pranta Roy",
            "email": "roypranta97@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Vendor\\Afropay\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vendor\\Afropay\\AfropayServiceProvider"
            ],
            "aliases": {
                "Afropay": "Vendor\\Afropay\\Facades\\Afropay"
            }
        }
    }
}
